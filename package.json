{
  "name": "legion-capture",
  "version": "0.0.49",
  "description": "Express.js App to capture metrics.",
  "main": "src/index.js",
  "bin": "src/main.js",
  "scripts": {
    "start": "forever --spinSleepTime 250 --minUptime 1000 src/main.js",
    "inspector": "pouchdb-server --dir ./dbs",
    "test": "eslint ./src/** ./spec/** && jasmine && istanbul cover jasmine",
    "clean": "rimraf node_modules coverage .unit-test-dbs ./legion-capture.pouchdb"
  },
  "author": "Christopher Lane Hinson <lane@webperformance.com>",
  "license": "APSL-2.0",
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-plugin-promise": "^3.6.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.9.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "express": "^4.13.4",
    "forever": "^0.15.2",
    "fs-extra": "^4.0.1",
    "jsonschema": "^1.2.0",
    "legion-metrics": "https://github.com/lane-webperformance/legion-metrics#v0.0.49",
    "node-fetch": "^1.5.3",
    "promise-delay": "^2.1.0",
    "uuid": "^3.1.0"
  },
  "optionalDependencies": {
    "lokijs": "^1.5.1",
    "pouchdb": "^6.4.1",
    "pouchdb-upsert": "^2.2.0",
    "pouchdb-server": "^2.3.7"
  }
}
